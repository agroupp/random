{
    "compilerOptions": {
        "declaration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "module": "commonjs",
        "strict": true,
        "moduleResolution": "node",
        "strictNullChecks": true,
        "strictPropertyInitialization": true,
        "noImplicitAny": true,
        "strictFunctionTypes": true,
        "noFallthroughCasesInSwitch": true,
        "baseUrl": ".",
        "outDir": "./lib",
        "target": "ES2015"
    }
}